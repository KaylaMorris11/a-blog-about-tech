{{#each posts as |post| }}
<div>
     <div class="card">
      <div class="card-header">
    <h1>
        <a href="/api/posts/{{post.id}}">
        {{post.title}}
        </a>
    </h1>
    </div>
    </div>
    <div class="card-body">
    <p>
        Posted by {{post.user.username}} on {{format_date post.createdAt}}
        <a href="/api/posts/{{post.id}}"></a>
    </p>
    <div>
        </div>
</div>
    {{/each}}